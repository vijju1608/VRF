<UserControl x:Class="JCHVRF_New.Views.SystemDetails"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JCHVRF_New.Views"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:const="clr-namespace:JCHVRF_New.Common.Constants"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             xmlns:ext="clr-namespace:JCHVRF_New.Common.UI.Extensions"             
            ext:JCHDockerService.Title="{Lang:LocalisedText Key=SYSTEM_DETAILS}"
             ext:JCHDockerService.FloatingHeight="430"
             ext:JCHDockerService.FloatingWidth="620"
             xmlns:Lang="clr-namespace:JCHVRF_New.LanguageData"
             xmlns:conv1="clr-namespace:JCHVRF_New.Model"
             >
    <ContentControl x:Name="HvacSystemDetailsContent" prism:RegionManager.RegionName="{x:Static const:RegionNames.MasterDesignerSystemDetailsRegion}"/>
    <UserControl.Resources>
        <conv1:LanguageNameConverter x:Key="LangNameConverter" />
    </UserControl.Resources>
</UserControl>
