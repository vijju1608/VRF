                <Style TargetType="Grid">
                    <Setter Property="Visibility" Value="Collapsed"/>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=UserControl}, Path=ActualWidth, Converter={conv:DoubleGreaterThanParameterConverter}, ConverterParameter=300}" Value="True">
                            <Setter Property="Visibility" Value="Visible"/>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>

            </Grid.Style>

            <Grid Grid.Row="0" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="70*"></ColumnDefinition>
                    <ColumnDefinition Width="10*"></ColumnDefinition>
                    <ColumnDefinition Width="10*"></ColumnDefinition>
                    <ColumnDefinition Width="10*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <TextBlock Grid.Column="0" Text="Description" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                <StackPanel Orientation="Vertical" Grid.Column="1" >
                    <TextBlock  Text="Project" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                    <TextBlock  Text="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Grid}, Path=DataContext.LengthUnit,StringFormat='(\{0}\)'}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                </StackPanel>
                <StackPanel Orientation="Vertical" Grid.Column="2">
                    <TextBlock Text="Max" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                    <TextBlock Text="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Grid}, Path=DataContext.LengthUnit,StringFormat='(\{0}\)'}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                </StackPanel>
                <TextBlock Grid.Column="3" Text="Result" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
            </Grid>
            <Line  X1="0" Y1="0"  
                   X2="900" Y2="0"  
                   Stroke="Black"   
                   StrokeThickness="1"
                   Grid.Row="0" Grid.Column="1"  Grid.ColumnSpan="2" Height="1" VerticalAlignment="Bottom"></Line>

            <!--<Line  X1="0" Y1="0"  
                   X2="0" Y2="900"  
                   Stroke="DarkGray"  
                   StrokeThickness="1"
                   Grid.Column="0" Grid.Row="1"  Grid.RowSpan="4" Width="1"  HorizontalAlignment="Right"></Line>-->


            <Grid Grid.Row="1" Grid.Column="1" x:Name="grdLength">
                <ItemsControl ItemsSource="{Binding ListPipingInfoLength, UpdateSourceTrigger=PropertyChanged}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>

                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="70*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>

                                <TextBlock Grid.Column="0" Text="{Binding LongDescription}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <TextBlock Grid.Column="1" Text="{Binding Value}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <TextBlock Grid.Column="2" Text="{Binding Max}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <fa:ImageAwesome x:Name="imgAwesome" Grid.Column="3"  Icon="{Binding Path=IsValid, Converter={conv:BoolToFontAwesomeIconConverter}}"  Foreground="{Binding Path=IsValid,Converter={conv:BoolToSolidColorBrushConverter}}" Rotation="0" Height="10" Width="20" />
                            </Grid>
                            <DataTemplate.Triggers>
                                <DataTrigger  Binding="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=DataContext.IsValid}" Value="True">
                                    <Setter TargetName="imgAwesome"  Property="Visibility" Value="hidden"></Setter>
                                </DataTrigger>
                            </DataTemplate.Triggers>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
            </Grid>

            <Grid Grid.Row="2" Grid.Column="1" >
                <ItemsControl ItemsSource="{Binding ListPipingInfoHeight, UpdateSourceTrigger=PropertyChanged}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="70*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>

                                <TextBlock Grid.Column="0" Text="{Binding LongDescription}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <TextBlock Grid.Column="1" Text="{Binding Value}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <TextBlock Grid.Column="2" Text="{Binding Max}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <fa:ImageAwesome Grid.Column="3"  Icon="{Binding Path=IsValid, Converter={conv:BoolToFontAwesomeIconConverter}}"  Foreground="{Binding Path=IsValid,Converter={conv:BoolToSolidColorBrushConverter}}" Rotation="0" Height="10" Width="20" />
                            </Grid>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
            </Grid>
            
            <Grid Grid.Row="3" Grid.Column="1" >
                <ItemsControl ItemsSource="{Binding ListPipingInfoAdditional, UpdateSourceTrigger=PropertyChanged}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="70*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                    <ColumnDefinition Width="10*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
 
                                <TextBlock Grid.Column="0" Text="{Binding LongDescription}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <TextBlock Grid.Column="1" Text="{Binding ValueDescription}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <TextBlock Grid.Column="2" Text="{Binding MaxDescription}" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                                <fa:ImageAwesome Grid.Column="3"  Icon="{Binding Path=IsValid, Converter={conv:BoolToFontAwesomeIconConverter}}"  Foreground="{Binding Path=IsValid,Converter={conv:BoolToSolidColorBrushConverter}}" Rotation="0" Height="10" Width="20" />
                            </Grid>
                        </DataTemplate>     
                    </ItemsControl.ItemTemplate>
                </ItemsControl>

            </Grid>
          
            <TextBlock Grid.Column="0" Text="Length" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center"></TextBlock>
            <TextBlock Grid.Column="0" Text="Height" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center"></TextBlock>
        </Grid>

    </Grid>

</UserControl>
